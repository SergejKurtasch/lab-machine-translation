def create_optimized_sequences(corpus, max_vocab_size=5000, max_seq_len=15):
    """–°–æ–∑–¥–∞–Ω–∏–µ –æ–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–µ–π"""
    
    # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Å–ª–æ–≤–∞—Ä—è –¥–ª—è –ª—É—á—à–µ–π –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
    tokenizer = Tokenizer(num_words=max_vocab_size, oov_token='<OOV>')
    tokenizer.fit_on_texts(corpus)
    
    # –†–µ–∞–ª—å–Ω—ã–π —Ä–∞–∑–º–µ—Ä —Å–ª–æ–≤–∞—Ä—è
    total_words = min(len(tokenizer.word_index) + 1, max_vocab_size)
    print(f"Vocabulary size: {total_words}")
    
    # –°–æ–∑–¥–∞–µ–º –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –±–æ–ª–µ–µ —ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω–æ
    input_sequences = []
    for line in corpus:
        token_list = tokenizer.texts_to_sequences([line])[0]
        
        # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –¥–ª–∏–Ω—É –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        if len(token_list) > max_seq_len:
            token_list = token_list[:max_seq_len]
            
        # –°–æ–∑–¥–∞–µ–º n-–≥—Ä–∞–º–º—ã —Ç–æ–ª—å–∫–æ –¥–ª—è –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –¥–ª–∏–Ω–Ω—ã—Ö –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–µ–π
        if len(token_list) >= 3:
            for i in range(1, len(token_list)):
                n_gram_sequence = token_list[:i+1]
                if len(n_gram_sequence) <= max_seq_len:
                    input_sequences.append(n_gram_sequence)
    
    print(f"Created {len(input_sequences)} sequences")
    
    # –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ padding
    max_sequence_len = min(max([len(x) for x in input_sequences]), max_seq_len)
    input_sequences = np.array(pad_sequences(input_sequences, maxlen=max_sequence_len, padding='pre'))
    
    # –†–∞–∑–¥–µ–ª—è–µ–º –Ω–∞ –≤—Ö–æ–¥—ã –∏ –º–µ—Ç–∫–∏
    predictors = input_sequences[:,:-1]
    labels = input_sequences[:,-1]  # –ù–ï –∏—Å–ø–æ–ª—å–∑—É–µ–º categorical encoding –¥–ª—è —ç–∫–æ–Ω–æ–º–∏–∏ –ø–∞–º—è—Ç–∏
    
    print(f"Predictors shape: {predictors.shape}")
    print(f"Labels shape: {labels.shape}")
    print(f"Max sequence length: {max_sequence_len}")
    
    return tokenizer, predictors, labels, total_words, max_sequence_len

# –°–æ–∑–¥–∞–µ–º –æ–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ
tokenizer_opt, predictors_opt, labels_opt, total_words_opt, max_seq_len_opt = create_optimized_sequences(
    corpus, max_vocab_size=3000, max_seq_len=12
)# –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è –ø—Ä–µ–¥–æ–±—Ä–∞–±–æ—Ç–∫–∞ –¥–∞–Ω–Ω—ã—Ö —Å –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏–µ–º –æ—à–∏–±–æ–∫
def preprocess_text(text):
    """–£–ª—É—á—à–µ–Ω–Ω–∞—è –ø—Ä–µ–¥–æ–±—Ä–∞–±–æ—Ç–∫–∞ —Ç–µ–∫—Å—Ç–∞"""
    # –ò–°–ü–†–ê–í–õ–ï–ù–ò–ï: –ø—Ä–∞–≤–∏–ª—å–Ω–æ —Å–æ—Ö—Ä–∞–Ω—è–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–º–µ–Ω—ã
    text = text.replace("\r\n", "\n").replace("\r", "\n")
    
    # –£–¥–∞–ª—è–µ–º –ª–∏—à–Ω–∏–µ —Å–∏–º–≤–æ–ª—ã –∏ –Ω–æ—Ä–º–∞–ª–∏–∑—É–µ–º
    text = re.sub(r'[^\w\s\n.,!?;:\'-]', '', text)
    text = re.sub(r'\s+', ' ', text)  # –ú–Ω–æ–∂–µ—Å—Ç–≤–µ–Ω–Ω—ã–µ –ø—Ä–æ–±–µ–ª—ã –≤ –æ–¥–∏–Ω
    
    return text.lower().strip()

def create_corpus(text):
    """–°–æ–∑–¥–∞–Ω–∏–µ –∫–æ—Ä–ø—É—Å–∞ —Å —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–µ–π"""
    lines = text.split('\n')
    
    # –§–∏–ª—å—Ç—Ä—É–µ–º –ø—É—Å—Ç—ã–µ —Å—Ç—Ä–æ–∫–∏ –∏ —Å–ª–∏—à–∫–æ–º –∫–æ—Ä–æ—Ç–∫–∏–µ/–¥–ª–∏–Ω–Ω—ã–µ
    corpus = []
    for line in lines:
        line = line.strip()
        if len(line) > 10 and len(line.split()) >= 3:  # –ú–∏–Ω–∏–º—É–º 3 —Å–ª–æ–≤–∞
            corpus.append(line)
    
    print(f"Filtered corpus: {len(corpus)} lines from {len(lines)} total")
    return corpus

# –ó–∞–≥—Ä—É–∑–∫–∞ –∏ –ø—Ä–µ–¥–æ–±—Ä–∞–±–æ—Ç–∫–∞ –¥–∞–Ω–Ω—ã—Ö
url = "https://www.gutenberg.org/files/1041/1041-0.txt"
